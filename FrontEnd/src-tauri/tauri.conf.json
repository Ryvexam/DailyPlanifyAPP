{
  "productName": "dailyplanify",
  "version": "1.0.1",
  "identifier": "fr.dailyplanify.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev -- --host",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "dailyplanify",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],


    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}